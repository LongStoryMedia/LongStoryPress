    "react-svg-inline": "^2.1.1",
    "serialize-javascript": "^1.5.0",
    "snyk": "^1.316.1",
    "universal-cookie": "^3.0.4"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
}
